        "outDir": "./built/",
        "sourceMap": true,
        "strict": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "lib": [
            "dom",
            "dom.iterable",
            "es2015",
            "es2015.promise",
            "es2015.iterable",
